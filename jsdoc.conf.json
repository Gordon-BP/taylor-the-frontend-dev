{
    "plugins": ["node_modules/better-docs/typescript"],
    "recurseDepth": 10,
    "source": {
        "include":["src", "src/agents", "src/utils", "tests", "tests/utils"],
        "includePattern": ".+\\.[jt]s(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "node_modules/better-docs",
        "encoding": "utf8",             
        "destination": "./docs/",         
        "recurse": true
    }
}

